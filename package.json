{
  "name": "kpipe-core",
  "version": "0.10.0",
  "description": "Core IO and streams for kpipe",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run build-ts && npm run lint",
    "test": "./node_modules/.bin/jest --forceExit --coverage --verbose --detectOpenHandles --passWithNoTests",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "tsc --noEmit && eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "author": "Paul Rowan <prowan@novisci.com>",
  "license": "MIT",
  "repository": "github:novisci/kpipe-core",
  "devDependencies": {
    "@babel/parser": "^7.8.3",
    "@babel/types": "^7.8.3",
    "@types/jest": "^24.9.1",
    "@types/node": "^13.1.8",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "dotenv": "^8.2.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.9.0",
    "ts-jest": "^24.3.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "aws-sdk": "^2.624.0",
    "node-rdkafka": "^2.7.4",
    "node-typestream": "^0.1.2",
    "uid-safe": "^2.1.5"
  }
